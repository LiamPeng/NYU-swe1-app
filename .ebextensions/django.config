option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "mysite.settings"
    PYTHONPATH: "/var/app/current:$PYTHONPATH"

container_commands:
  01_migrate:
    command: "/var/app/venv/staging/bin/python manage.py migrate --noinput"
    leader_only: true

  02_collectstatic:
    command: "/var/app/venv/staging/bin/python manage.py collectstatic --noinput"
    leader_only: true